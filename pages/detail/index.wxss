.detail-wrap {
  width: 100%;
  height: 100%;
}
.detail-top {
  width: 100%;
  height: 120rpx;
  background-color: #fddb43;
  box-sizing: border-box;
  padding: 0rpx 40rpx;
  display: flex;
  align-items: center;
  justify-content: space-between;
}
.detail-picker {
  width: 230rpx;
  height: 80rpx;
  line-height: 80rpx;
}
.detail-trian {
  width: 28rpx;
  height: 28rpx;
  vertical-align: -4rpx;
}
.detail-use-time {
  width: 400rpx;
  height: 100%;
  display: flex;
  align-items: center;
  justify-content: space-between;
  text-align: center;
}
.detail-content {
  width: 100%;
}
.detail-date {
  width: 100%;
  height: 80rpx;
  color: rgb(151, 151, 151);
  font-size: 32rpx;
  border-bottom: 1rpx solid rgb(235, 235, 235);
  display: flex;
  align-items: center;
  justify-content: space-between;
  box-sizing: border-box;
  padding: 0rpx 40rpx;
}
.detail-list {
  width: 100%;
  height: 120rpx;
  box-sizing: border-box;
  padding: 0rpx 40rpx;
  display: flex;
  align-items: center;
  justify-content: space-between;
}
.detail-list-item {
  width: 100%;
  height: 100%;
  border-bottom: 1rpx solid rgb(235, 235, 235);
  display: flex;
  align-items: center;
  justify-content: space-between;
  margin-left: 40rpx;
}
.detail-title {
  font-size: 40rpx;
  color: #000;
  font-size: 32rpx;
}
.detail-image {
  width: 60rpx;
  height: 60rpx;
}
.detail-arrow {
  width: 40rpx;
  height: 40rpx;
}
